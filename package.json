{
  "name": "grapesjs-react-renderer",
  "version": "1.0.0",
  "description": "A GrapesJS plugin for rendering React components in the canvas with full prop support, TypeScript types, and provider integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./helpers": {
      "import": "./dist/helpers.esm.js",
      "require": "./dist/helpers.js",
      "types": "./dist/helpers.d.ts"
    },
    "./factories": {
      "import": "./dist/factories.esm.js",
      "require": "./dist/factories.js",
      "types": "./dist/factories.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "grapesjs",
    "react",
    "plugin",
    "page-builder",
    "components",
    "visual-editor",
    "typescript",
    "redux",
    "providers"
  ],
  "author": "David Werth (@dwrth)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "grapesjs": "^0.21.3"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
